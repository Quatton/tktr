{
  "name": "@tktr/core",
  "version": "0.1.0",
  "description": "Core library for Tickettier",
  "repository": {
    "url": "git+https://github.com/Quatton/tktr.git"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "stripe",
    "ticket",
    "wrapper",
    "ecommerce",
    "tier",
    "subscription",
    "event"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "Quatton",
  "license": "Apache-2.0",
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
